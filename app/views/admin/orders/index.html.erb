<div class="container">
	<table class = "table table-striped">
		<thead>
			<tr>
				<th>購入日時</th>
				<th>購入者</th>
				<th>注文個数</th>
				<th>注文ステータス</th>
			</tr>
		</thead>
		<tbody>
				<% @end_users.each do |end_user| %>
					<% end_user.orders.each do |order| %>
						<tr>
							<td><%= link_to order.created_at, admin_order_path(order) %></td>
							<td><%= order.end_user.last_name %> <%= order.end_user.first_name %><br></td>
							<td><%= order.order_details.count %></td>
							<td><%= order.status %></td>
						</tr>
					<% end %>
				<% end %>
		</tbody>
	</table>
</div>