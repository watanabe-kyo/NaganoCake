
<div class="container">
	<div class="col-xs-6">
	<%= form_for(@end_user, url: end_user_path(@end_user)) do |f| %>
		<p>性</p>
		<%= f.text_field :last_name, autofocus: true, class:"form-control", id:"inputEmail", placeholder:"性"%>
		<p>名</p>
		<%= f.text_field :first_name, class: 'form-control', placeholder:"名" %>
		<p>セイ</p>
		<%= f.text_field :last_name_kana, class: 'form-control', placeholder:"セイ" %>
		<p>メイ</p>
		<%= f.text_field :first_name_kana, class: 'form-control', placeholder:"メイ" %>
		<p>メールアドレス</p>
		<%= f.text_field :email, class: 'form-control', placeholder:"メールアドレス" %>
		<p>郵便番号</p>
		<%= f.number_field :postal_code, class: 'form-control', placeholder:"郵便番号" %>
		<p>住所</p>
		<%= f.text_area :address, class: 'form-control', placeholder:"住所" %>
		<p>電話番号</p>
		<%= f.number_field :telephone_number, class: 'form-control', placeholder:"電話番号" %>


		<%= f.submit '編集内容を保存する', class: "btn btn-info" %>

		<%= link_to '退会', end_users_withdraw_path, class: "btn btn-danger" %>
	<% end %>
	</div>
</div>
